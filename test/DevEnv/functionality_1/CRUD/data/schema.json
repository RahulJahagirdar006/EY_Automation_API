{
    "candidate_schema":{
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          },
          "candidates": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "candidate_id": {
                  "type": "number"
                },
                "company_id": {
                  "type": "number"
                },
                "first_name": {
                  "type": "string"
                },
                "last_name": {
                  "type": "string"
                },
                "profilePicURL": {
                  "type": "string"
                },
                "emp_id": {
                  "type": "number"
                },
                "user_id": {
                  "type": "number"
                },
                "createdAt": {
                  "type": "string"
                },
                "updatedAt": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
}